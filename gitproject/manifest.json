{
  "manifest_version": 3,
  "name": "Crypto Tracker Pro",
  "version": "1.0",
  "description": "Extension to track crypto prices and personal holdings across platforms.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["storage"],
  "host_permissions": ["https://api.coingecko.com/*"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
